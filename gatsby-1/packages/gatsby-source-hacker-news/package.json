    "axios": "^0.21.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "babel-preset-gatsby-package": "^1.10.0-next.1",
    "cross-env": "^7.0.3"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-source-hacker-news#readme",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin"
  ],
  "license": "MIT",
  "peerDependencies": {
    "gatsby": "^3.0.0-next.0"
  },
  "repository": {
    "type": "git",
